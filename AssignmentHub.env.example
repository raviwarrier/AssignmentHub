# AssignmentHub Environment Configuration Template
# Copy this file to AssignmentHub.env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/assignmenthub

# Session Security
SESSION_SECRET=your-long-random-session-secret-key-here

# Admin Access
ADMIN_PASSWORD=your-secure-admin-password

# Team Login Credentials
TEAM_1_PASSWORD=team1-password
TEAM_2_PASSWORD=team2-password
TEAM_3_PASSWORD=team3-password
TEAM_4_PASSWORD=team4-password
TEAM_5_PASSWORD=team5-password
TEAM_6_PASSWORD=team6-password
TEAM_7_PASSWORD=team7-password
TEAM_8_PASSWORD=team8-password
TEAM_9_PASSWORD=team9-password

# Optional: Delete Password (if different from admin)
ADMIN_DELETE_PASSWORD=your-secure-delete-password